<%
  //app version
  if(!version)
  {
    version="0.0.0";
  }
  //open tab
  if(!tab)
  {
    tab="main";
  }
%>
<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width,initial-scale=1.0">
    <meta name="theme-color" content="#2196F3">
    <link rel="preload" href="../static/style/student-dashboard/style.css" as="style">
    <link rel="preload" href="../static/style/student-dashboard/dash.css" as="style">
    <link rel="preload" href="../static/script/student-dashboard/data.js" as="script">
    <link rel="preload" href="../static/script/student-dashboard/script.js" as="script">
    <link rel="stylesheet" href="../static/style/student-dashboard/style.css">
    <link rel="stylesheet" href="../static/style/student-dashboard/dash.css">
    <script src="../static/script/student-dashboard/data.js"></script>
    <script src="../static/script/student-dashboard/script.js"></script>
    <title>Dashboard</title>
  </head>
  <body onresize="resizeSearch();">
    <div class="nav" id="topbar">
      <div class="menu" id="menu" tabindex="0" onclick="toggleMenu();">
        <div class="wrapper">
          <div class="box"></div>
          <div class="box"></div>
          <div class="box"></div>
        </div>
      </div>
      <div class="searchcontainer" id="searchcontainer">
        <input class="search" placeholder="Search" spellcheck="false" oninput="searchItem(event,this);" onclick="searchItem(null,this);"><!--There's another one in the side bar-->
      </div>
      <div class="app" onclick="window.location='/home';">
        <div class="icon"></div>
        <div class="wrapper">
          <div class="title">
            <div class="name">PPMS</div>
            <div class="version"><%=version%></div>
          </div>
          <div class="subtitle">Campus Placement Prediction &amp; Management System</div>
        </div>
      </div>
    </div>
    <div class="container">
      <div class="options" id="options"></div>
      <div class="contents">
        <div class="body" id="body" openTab="<%=tab%>">
          <!-- Contents will be dynamically loaded -->
          <div class="loading"></div>
          <!-- If the number of items are multiples of both 2 and 3 the better -->
        </div>
        <div class="seperator" id="seperator" onclick="hideMenu();" style="display: none;"></div>
        <div class="side" id="side">
          <div class="searchcontainer" id="searchcontainermin">
            <input class="search" placeholder="Search" spellcheck="false" oninput="searchItem(event,this);" onclick="searchItem(null,this);"><!--There's another one in the nav bar-->
          </div>
          <div class="option" loadtab="main">
            <div class="icon">
              <svg xmlns="http://www.w3.org/2000/svg" width="44" height="44" viewBox="0 0 24 24" fill="none" stroke="#ffffff" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round" class="feather feather-home"><path d="M3 9l9-7 9 7v11a2 2 0 0 1-2 2H5a2 2 0 0 1-2-2z"></path><polyline points="9 22 9 12 15 12 15 22"></polyline></svg>
            </div>
            <div class="wrapper">
              <div class="title">Home</div>
              <div class="subtitle">Main Home page</div>
            </div>
          </div>
          <div class="option" loadtab="profile">
            <div class="icon">
              <svg xmlns="http://www.w3.org/2000/svg" width="44" height="44" viewBox="0 0 24 24" fill="none" stroke="#ffffff" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round" class="feather feather-user"><path d="M20 21v-2a4 4 0 0 0-4-4H8a4 4 0 0 0-4 4v2"></path><circle cx="12" cy="7" r="4"></circle></svg>
            </div>
            <div class="wrapper">
              <div class="title">Profile</div>
              <div class="subtitle">Your profile</div>
            </div>
          </div>
          <div class="option" loadtab="option1">
            <div class="icon">
              <svg xmlns="http://www.w3.org/2000/svg" width="44" height="44" viewBox="0 0 24 24" fill="none" stroke="#ffffff" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round" class="feather feather-x"><line x1="18" y1="6" x2="6" y2="18"></line><line x1="6" y1="6" x2="18" y2="18"></line></svg>
            </div>
            <div class="wrapper">
              <div class="title">Option 1</div>
              <div class="subtitle">options</div>
            </div>
          </div>
          <div class="option" loadtab="option2">
            <div class="icon">
              <svg xmlns="http://www.w3.org/2000/svg" width="44" height="44" viewBox="0 0 24 24" fill="none" stroke="#ffffff" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round" class="feather feather-x"><line x1="18" y1="6" x2="6" y2="18"></line><line x1="6" y1="6" x2="18" y2="18"></line></svg>
            </div>
            <div class="wrapper">
              <div class="title">Option 2</div>
              <div class="subtitle">options</div>
            </div>
          </div>
          <div class="option" loadtab="option3">
            <div class="icon">
              <svg xmlns="http://www.w3.org/2000/svg" width="44" height="44" viewBox="0 0 24 24" fill="none" stroke="#ffffff" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round" class="feather feather-x"><line x1="18" y1="6" x2="6" y2="18"></line><line x1="6" y1="6" x2="18" y2="18"></line></svg>
            </div>
            <div class="wrapper">
              <div class="title">Option 3</div>
              <div class="subtitle">option</div>
            </div>
          </div>
          <div class="option" loadtab="option4">
            <div class="icon">
              <svg xmlns="http://www.w3.org/2000/svg" width="44" height="44" viewBox="0 0 24 24" fill="none" stroke="#ffffff" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round" class="feather feather-x"><line x1="18" y1="6" x2="6" y2="18"></line><line x1="6" y1="6" x2="18" y2="18"></line></svg>
            </div>
            <div class="wrapper">
              <div class="title">Option 4</div>
              <div class="subtitle">options</div>
            </div>
          </div>
          <div class="option" loadtab="option5">
            <div class="icon">
              <svg xmlns="http://www.w3.org/2000/svg" width="44" height="44" viewBox="0 0 24 24" fill="none" stroke="#ffffff" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round" class="feather feather-x"><line x1="18" y1="6" x2="6" y2="18"></line><line x1="6" y1="6" x2="18" y2="18"></line></svg>
            </div>
            <div class="wrapper">
              <div class="title">Option 5</div>
              <div class="subtitle">options</div>
            </div>
          </div>
          <div class="option" loadtab="option6">
            <div class="icon">
              <svg xmlns="http://www.w3.org/2000/svg" width="44" height="44" viewBox="0 0 24 24" fill="none" stroke="#ffffff" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round" class="feather feather-x"><line x1="18" y1="6" x2="6" y2="18"></line><line x1="6" y1="6" x2="18" y2="18"></line></svg>
            </div>
            <div class="wrapper">
              <div class="title">Option 6</div>
              <div class="subtitle">options</div>
            </div>
          </div>
          <div class="option" loadtab="option6">
            <div class="icon">
              <svg xmlns="http://www.w3.org/2000/svg" width="44" height="44" viewBox="0 0 24 24" fill="none" stroke="#ffffff" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round" class="feather feather-x"><line x1="18" y1="6" x2="6" y2="18"></line><line x1="6" y1="6" x2="18" y2="18"></line></svg>
            </div>
            <div class="wrapper">
              <div class="title">Settings</div>
              <div class="subtitle">settings</div>
            </div>
          </div>
        </div>
      </div>
    </div>
    <script src="../static/plugins/chart.js/Chart.min.js"></script>
    <script src="../static/plugins/chart.js/utils.js"></script>
  </body>
</html>
