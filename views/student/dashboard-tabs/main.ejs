      <div class="overlay" onclick="alert('in development');console.log('edit profile pic in development')">
        <div class="editwrapper">
          <div class="icon"></div>
          <div class="title">Edit</div>
        </div>
      </div>
    </div>
    <div class="details">
      <div class="wrapper">
        <div class="name"><%-name%></div>
        <div class="course"><%-course%></div>
        <div class="email"><%-email%></div>
        <div class="phone"><%-phone%></div>
        <div class="bio"><%-about%></div>
      </div>
    </div>
    <div class="edit" onclick="alert('in development');console.log('edit profile in development')"></div>
