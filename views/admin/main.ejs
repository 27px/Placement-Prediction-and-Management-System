<div class="dashboard second-dashboard">
  <div class="wide">
    <div class="icon-list">
      <a href="/home" class="icon-wrapper">
        <div class="icon">
          <svg width="44" height="44" viewBox="0 0 24 24" fill="none" stroke="#000000" stroke-width="1.2" stroke-linecap="round" stroke-linejoin="round"><path d="M3 9l9-7 9 7v11a2 2 0 0 1-2 2H5a2 2 0 0 1-2-2z"></path><polyline points="9 22 9 12 15 12 15 22"></polyline></svg>
        </div>
        <div class="title">Home</div>
        <div class="subtitle">Main Home</div>
      </a>
      <a href="/statistics" class="icon-wrapper">
        <div class="icon">
          <svg width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="#000000" stroke-width="1.2" stroke-linecap="round" stroke-linejoin="round"><polyline points="22 12 18 12 15 21 9 3 6 12 2 12"></polyline></svg>
        </div>
        <div class="title">Statistics</div>
        <div class="subtitle">Visualize</div>
      </a>
      <a href="/gallery" class="icon-wrapper">
        <div class="icon">
          <svg width="44" height="44" viewBox="0 0 24 24" fill="none" stroke="#000000" stroke-width="1.2" stroke-linecap="round" stroke-linejoin="round"><rect x="3" y="3" width="18" height="18" rx="2" ry="2"></rect><circle cx="8.5" cy="8.5" r="1.5"></circle><polyline points="21 15 16 10 5 21"></polyline></svg>
        </div>
        <div class="title">Gallery</div>
        <div class="subtitle">Pictures</div>
      </a>
      <a href="/team" class="icon-wrapper">
        <div class="icon">
          <svg width="44" height="44" viewBox="0 0 24 24" fill="none" stroke="#000000" stroke-width="1.2" stroke-linecap="round" stroke-linejoin="round"><path d="M17 21v-2a4 4 0 0 0-4-4H5a4 4 0 0 0-4 4v2"></path><circle cx="9" cy="7" r="4"></circle><path d="M23 21v-2a4 4 0 0 0-3-3.87"></path><path d="M16 3.13a4 4 0 0 1 0 7.75"></path></svg>
        </div>
        <div class="title">Team</div>
        <div class="subtitle">Coordinators</div>
      </a>
      <a href="/contact" class="icon-wrapper">
        <div class="icon">
          <svg width="44" height="44" viewBox="0 0 24 24" fill="none" stroke="#000000" stroke-width="1.2" stroke-linecap="round" stroke-linejoin="round"><path d="M15.05 5A5 5 0 0 1 19 8.95M15.05 1A9 9 0 0 1 23 8.94m-1 7.98v3a2 2 0 0 1-2.18 2 19.79 19.79 0 0 1-8.63-3.07 19.5 19.5 0 0 1-6-6 19.79 19.79 0 0 1-3.07-8.67A2 2 0 0 1 4.11 2h3a2 2 0 0 1 2 1.72 12.84 12.84 0 0 0 .7 2.81 2 2 0 0 1-.45 2.11L8.09 9.91a16 16 0 0 0 6 6l1.27-1.27a2 2 0 0 1 2.11-.45 12.84 12.84 0 0 0 2.81.7A2 2 0 0 1 22 16.92z"></path></svg>
        </div>
        <div class="title">Contact</div>
        <div class="subtitle">Help</div>
      </a>
      <a href="/logout" class="icon-wrapper">
        <div class="icon">
          <svg width="44" height="44" viewBox="0 0 24 24" fill="none" stroke="#000000" stroke-width="1.2" stroke-linecap="round" stroke-linejoin="round"><path d="M9 21H5a2 2 0 0 1-2-2V5a2 2 0 0 1 2-2h4"></path><polyline points="16 17 21 12 16 7"></polyline><line x1="21" y1="12" x2="9" y2="12"></line></svg>
        </div>
        <div class="title">Logout</div>
        <div class="subtitle">Exit</div>
      </a>
    </div>
    <div class="bottom-container">
      <div class="subcard rounded equal link" data-target="dump-data">
        <div class="icon">
          <svg width="44" height="44" viewBox="0 0 24 24" fill="none" stroke="#000000" stroke-width="1.2" stroke-linecap="round" stroke-linejoin="round"><polyline points="21 8 21 21 3 21 3 8"></polyline><rect x="1" y="3" width="22" height="5"></rect><line x1="10" y1="12" x2="14" y2="12"></line></svg>
        </div>
        <div class="content">
          <div class="title title-large">Dump Data</div>
          <div class="title">Delete Data each year</div>
        </div>
      </div>
      <div class="subcard rounded equal link" data-target="add-coordinator">
        <div class="icon">
          <svg width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="#000000" stroke-width="1.2" stroke-linecap="round" stroke-linejoin="round"><polygon points="12 2 15.09 8.26 22 9.27 17 14.14 18.18 21.02 12 17.77 5.82 21.02 7 14.14 2 9.27 8.91 8.26 12 2"></polygon></svg>
        </div>
        <div class="content">
          <div class="title title-large">Select Coordinators</div>
          <div class="title">Select Coordinators from students</div>
        </div>
      </div>
    </div>
    <div class="listcontainer">
      <div class="card subgrid">
        <div class="subcard rounded link" data-target="schedule">
          <div class="icon">
            <svg width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="#000000" stroke-width="1.2" stroke-linecap="round" stroke-linejoin="round"><rect x="3" y="4" width="18" height="18" rx="2" ry="2"></rect><line x1="16" y1="2" x2="16" y2="6"></line><line x1="8" y1="2" x2="8" y2="6"></line><line x1="3" y1="10" x2="21" y2="10"></line></svg>
          </div>
          <div class="content">
            <div class="title title-large">Schedule Event</div>
            <div class="title">Approve Schedule</div>
          </div>
        </div>
        <div class="subcard rounded link" data-target="add-department">
          <div class="icon">
            <svg width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="#000000" stroke-width="1.2" stroke-linecap="round" stroke-linejoin="round"><circle cx="12" cy="12" r="10"></circle><line x1="12" y1="8" x2="12" y2="16"></line><line x1="8" y1="12" x2="16" y2="12"></line></svg>
          </div>
          <div class="content">
            <div class="title title-large">Add Department</div>
            <div class="title">Add new Department</div>
          </div>
        </div>
        <div class="subcard rounded link" data-target="dump-data">
          <div class="icon">
            <svg width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="#000000" stroke-width="1.2" stroke-linecap="round" stroke-linejoin="round"><circle cx="12" cy="8" r="7"></circle><polyline points="8.21 13.89 7 23 12 20 17 23 15.79 13.88"></polyline></svg>
          </div>
          <div class="content">
            <div class="title title-large">Machine Learning</div>
            <div class="title">Train ML Model</div>
          </div>
        </div>
      </div>
      <div class="card rounded white-light link" data-target="add-course">
        <svg width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="#000000" stroke-width="0.6" stroke-linecap="round" stroke-linejoin="round"><rect x="3" y="3" width="18" height="18" rx="2" ry="2"></rect><line x1="12" y1="8" x2="12" y2="16"></line><line x1="8" y1="12" x2="16" y2="12"></line></svg>
        <div class="title">Add Course</div>
      </div>
      <div class="card rounded bg-highlighted link" data-target="add-company">
        <svg width="44" height="44" viewBox="0 0 24 24" fill="none" stroke="#ffffff" stroke-width="0.8" stroke-linecap="round" stroke-linejoin="round"><path d="M16 21v-2a4 4 0 0 0-4-4H5a4 4 0 0 0-4 4v2"></path><circle cx="8.5" cy="7" r="4"></circle><line x1="20" y1="8" x2="20" y2="14"></line><line x1="23" y1="11" x2="17" y2="11"></line></svg>
        <div class="title">Add Company</div>
      </div>
    </div>
  </div>
  <div class="small-container">
    <div class="notificationcontainer">
      <div class="notifytitle">Messages</div>
      <div class="notifycontainer">
        <!--Notifications-->
        <%
          if(notifications.length>0)
          {
            notifications.forEach(message=>{
              %>
                <div class="notification notification-<%-message.type%>">
                  <div class="title"><%-message.name%></div>
                  <div class="desc"><%-message.message%></div>
                  <div class="from"><%-message.from%></div>
                  <div class="time"><%-new Date(message.date).toLocaleDateString("en-US")%></div>
                </div>
              <%
            });
          }
          else
          {
            %>
              <div class="notification notification-general">
                <div class="title">Bot</div>
                <div class="desc">You have no notifications</div>
                <div class="from">System</div>
                <div class="time"><%-new Date().toLocaleDateString("en-US")%></div>
              </div>
            <%
          }
        %>
        <!--Notifications-->
      </div>
    </div>
    <div class="gradcontainer grad" onclick="window.location='/about';">
      <div class="mainwrapper">
          <div class="icon">
            <svg width="44" height="44" viewBox="0 0 24 24" fill="none" stroke="#ffffff" stroke-width="1" stroke-linecap="round" stroke-linejoin="round"><circle cx="12" cy="12" r="10"></circle><line x1="12" y1="16" x2="12" y2="12"></line><line x1="12" y1="8" x2="12.01" y2="8"></line></svg>
          </div>
          <div class="wrapper">
            <div class="title">About Us</div>
            <div class="desc">Group - 12</div>
          </div>
      </div>
    </div>
  </div>
</div>
